<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LearningChessWebServer</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<display-name>AnalyzeGameHistoryServlet</display-name>
  	<servlet-name>AnalyzeGameHistoryServlet</servlet-name>
  	<servlet-class>AnalyzeGameHistoryServlet</servlet-class>
  </servlet>
  <servlet>
  	<display-name>GetMoveServlet</display-name>
  	<servlet-name>GetMoveServlet</servlet-name>
  	<servlet-class>GetMoveServlet</servlet-class>
  </servlet>
  <servlet>
  	<display-name>GameStateInfoServlet</display-name>
  	<servlet-name>GameStateInfoServlet</servlet-name>
  	<servlet-class>GameStateInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GameStateInfoServlet</servlet-name>
    <url-pattern>/gamestateinfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetMoveServlet</servlet-name>
    <url-pattern>/getmove</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AnalyzeGameHistoryServlet</servlet-name>
    <url-pattern>/analyzehistory</url-pattern>
  </servlet-mapping>
   <context-param>
      <param-name>FILE_NAME</param-name>
      <param-value>LearningEnginePersistence</param-value>
    </context-param>
    <context-param>
      <param-name>CACHE_SIZE</param-name>
      <param-value>1000000000</param-value>
    </context-param>
    <context-param>
      <param-name>performInitialization</param-name>
      <param-value>0</param-value>
    </context-param>
  <listener>
  	<listener-class>
  		LearningEngineServletContextListener
  	</listener-class>
  </listener>
</web-app>